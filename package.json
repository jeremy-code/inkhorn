{
  "name": "inkhorn",
  "description": "an open-source next-gen notetaking app for students",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "next",
    "typescript",
    "react",
    "drizzle",
    "drizzle-kit",
    "panda",
    "pandacss",
    "vercel"
  ],
  "license": "MIT",
  "author": "Jeremy Nguyen <hi@jeremy.ng> (https://jeremy.ng)",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremy-code/inkhorn.git"
  },
  "homepage": "https://github.com/jeremy-code/inkhorn#readme",
  "scripts": {
    "postinstall": "husky install && panda codegen",
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier 'src/**/*.{js,jsx,ts,tsx,json}' --write --ignore-path .gitignore",
    "db:push": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx ./scripts/migrate.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@ark-ui/react": "^1.3.0",
    "@auth/core": "^0.22.0",
    "@auth/drizzle-adapter": "^0.3.16",
    "@next/bundle-analyzer": "canary",
    "@react-aria/datepicker": "^3.9.1",
    "@react-aria/i18n": "^3.10.0",
    "@react-stately/datepicker": "^3.9.1",
    "@tanstack/react-virtual": "^3.0.2",
    "@tiptap/core": "^2.1.16",
    "@tiptap/pm": "^2.1.16",
    "@tiptap/react": "^2.1.16",
    "@tiptap/starter-kit": "^2.1.16",
    "@vercel/analytics": "^1.1.2",
    "@vercel/speed-insights": "^1.0.7",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.29.3",
    "drizzle-valibot": "^0.1.1",
    "lucide-react": "^0.314.0",
    "luxon": "^3.4.4",
    "next": "canary",
    "next-auth": "beta",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1",
    "sqids": "^0.3.0",
    "valibot": "^0.27.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@pandacss/dev": "^0.28.0",
    "@pandacss/types": "^0.28.0",
    "@park-ui/panda-preset": "^0.30.1",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.6",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@typescript-eslint/utils": "^6.19.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.0.2+sha256.825003a0f561ad09a3b1ac4a3b3ea6207af2796d54f62a9420520915721f5186",
  "dependenciesMeta": {
    "@ianvs/prettier-plugin-sort-imports@4.1.1": {
      "unplugged": true
    }
  }
}
